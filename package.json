    "knip": "^5.42.0",
    "postcss": "^8.5.0",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "tailwindcss": "^3.4.17",
    "ts-node": "^10.9.2",
