    "agora-token": "^2.0.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.471.1",
    "next": "^15.1.4",
    "prettier-eslint": "^16.3.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "^5.0.0",
